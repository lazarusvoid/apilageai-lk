function toggleMCQSection(){const e=document.getElementById("mcq-challenges-list");e.style.display="none"===e.style.display?"block":"none"}document.addEventListener("DOMContentLoaded",(()=>{fetch("https://apilageai.lk/api/sideacess.php").then((e=>e.json())).then((e=>{const n=document.getElementById("mcq-challenges-list");n.innerHTML="",e.games?Object.entries(e.games).forEach((([e,t])=>{const a=Object.values(t.players||{})[0]?.gamerName||"Unknown",s=t.settings?.grade||"",l=t.settings?.subject||"",i=document.createElement("div");i.className="sidebar-item-wrapper",i.innerHTML=`\n  <div class="sidebar-item">\n    <a href="https://apilageai.lk/app?gameId=${e}" style="display: flex; align-items: center;">\n        <i class="fa-solid fa-gamepad" style="margin-right: 8px;"></i>\n        <span>G-${s} ${l} By ${a}</span>\n    </a>\n</div>\n                `,n.appendChild(i)})):n.innerHTML="<p>No MCQ challenges found.</p>"})).catch((e=>{console.error("Error loading MCQ challenges:",e),document.getElementById("mcq-challenges-list").innerHTML="<p style='color:red;'>Failed to load challenges.</p>"}))}));